{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": false,
	"printWidth": 80,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true,
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"printWidth": 200
			}
		}
	]
}
